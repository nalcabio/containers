#!/usr/bin/env bash
# -*- coding: utf-8; mode: sh -*-
set -euo pipefail
IFS=$'\n\t'

THIS="$(readlink -f "$0")"
readonly THIS

# HERE="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
# readonly HERE

function on_exit() {
  errcode="$1"
}

trap 'on_exit $?' EXIT

function on_error() {
  errcode="$1"
  linenum="$2"
  echo 1>&2 "[ERROR] ${THIS}: errcode: ${errcode} linenum: ${linenum}"
}

trap 'on_error $? ${LINENO}' ERR

superset fab create-admin "$@"
